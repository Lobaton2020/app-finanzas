{
  "name": "app-finanzas",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/app.ts",
  "scripts": {
    "start": "set TYPE=PROD&&node ./dist/app.js",
    "dev": "ts-node-dev ./src/app.ts",
    "build": "tsc",
    "build:docker": "docker build -t app-finanzas .",
    "test:unit": "jest --verbose",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/e2e/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js ",
    "typeorm:schema:drop": "npm run typeorm schema:drop",
    "typeorm:migration:run": "npm run typeorm migration:run",
    "typeorm:migration:generate": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate --name "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/types": "^27.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "jest": "^27.2.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mysql2": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37"
  }
}